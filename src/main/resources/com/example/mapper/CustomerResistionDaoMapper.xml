<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.CustomerRegistionDaoMapper">
	<insert id="Register" parameterType="CustomerDto">

		INSERT INTO 
		CUSTOMERINFO
		(ID,NAME,NAMEKN,TEL,POSTCODE,ADDRESS) 
		VALUES
		(#{customerId},#{customerName},#{customerNameKn},#{customerTelephoneNumber},#{customerPostCode},#{customerAddress})
	
	</insert>
</mapper>